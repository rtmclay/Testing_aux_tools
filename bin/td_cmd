#!/usr/bin/env lua
-- -*- lua -*-

_G._DEBUG          = false
local sort         = table.sort
local posix        = require("posix")
Version = "0.0"

require("strict")
require("capture")
require("fileOps")

function main()
   if (not isDir("./t1")) then
      io.stderr:write("Not in correct directory -> exiting\n")
      return
   end

   local fileA = posix.dirent.dir("./t1")
   sort(fileA)
   local lastFn = fileA[#fileA]
   if ( lastFn:sub(1,1) == "." ) then
      io.stderr:write("No test directory found -> exiting\n")
      return
   end

   local s = "cd t1/"..lastFn.."\n"
   
   io.stderr:write(s)
   io.stdout:write(s)
end


main()
